<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="thirdparty" extends="json-default,struts-default" >
		<global-results>
			<result name="loginerror">/backend/login.html</result>
		</global-results>
		<global-exception-mappings>
			<exception-mapping exception="com.yami.exception.NoPermissionException"
							   result="loginerror"/>
		</global-exception-mappings>

		<action name="thirdparty.*" class="com.yami.action.thirdparty.ThirdPartyAction" method="{1}">
			<result type="json">
				<param name="root">
					results
				</param>
			</result>
		</action>
	</package>
</struts>
